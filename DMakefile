DOTDOT=
DIR=
DIRS=$(DIR)echown $(DIR)edumpfs $(DIR)etouch $(DIR)newfs $(DIR)bffstool $(DIR)dcp $(DIR)eln $(DIR)fsck $(DIR)rdb $(DIR)tunefs $(DIR)echmod $(DIR)els $(DIR)mknod $(DIR)restore $(DIR)dumpfs $(DIR)diskpart $(DIR)fs $(DIR)fs $(DIR)test
DUMMYDIRS=$(DIRS:"*":"*.dummy")
CLEANDIRS=$(DIRS:"*":"*.clean")

all: $(DUMMYDIRS)

clean: $(CLEANDIRS)

$(DUMMYDIRS) : $(DUMMYDIRS)
	cd %(left:*.dummy:*)
	@dmake -q
	@cd

$(CLEANDIRS) : $(CLEANDIRS)
	cd %(left:*.clean:*)
	@dmake -q clean
	@cd
