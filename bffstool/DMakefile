ARCH=ami
DOTDOT=
#PROG=$(DOTDOT)/bin/bffstool.$(ARCH)
PROG=$(DOTDOT)/bin/bffstool
OBJ=$(DOTDOT)/obj.$(ARCH)/

OBJS=$(OBJ)toolres.o $(OBJ)tool.o $(OBJ)comm.o

$(PROG): $(OBJS)
	dcc -o $(PROG) $(OBJS)

$(OBJ)toolres.o: toolres.c
	dcc -c toolres.c -o %(left)

$(OBJ)tool.o: tool.c tool.h comm.h
	dcc -c tool.c -o %(left)


$(OBJ)comm.o: comm.c comm.h
	dcc -c comm.c -o %(left)

