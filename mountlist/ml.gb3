/* 3GB filesystem */
GB3:
    FileSystem = fs:BFFSFileSystem.debug
    Device = uaehf.device
    Unit = 0
    Sectorsize = 512
    LowCyl = 0
    HighCyl = 8191
    Surfaces = 3
    BlocksPerTrack = 256
    Stacksize = 512
    Flags = 0
    Mount = 1
    Priority = -1
    GlobVec = -1
    Buffers = 64
    Reserved = 0;  /* Unix partition */
    PreAlloc = 0xf800
#
